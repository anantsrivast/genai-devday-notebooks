{
    "name": "GenAI Jupyter Notebooks with MongoDB",
    "dockerComposeFile": "docker-compose.yml",
    "service": "lab-runner",
    "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
    "forwardPorts": [
        8888,
        27017
    ],
    "hostRequirements": {
        "cpus": 4
    },
    "containerEnv": {
        "PROXY_ENDPOINT": "https://vtqjvgchmwcjwsrela2oyhlegu0hwqnw.lambda-url.us-west-2.on.aws/",
        "MONGODB_URI": "mongodb://admin:mongodb@mongodb:27017/"
    },
   "postCreateCommand": "sudo apt-get update && sudo apt-get install -y git && pip install -r requirements.txt",
    "customizations": {
        "codespaces": {
            "openFiles": []
        },
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-toolsai.jupyter",
                "mongodb.mongodb-vscode"
            ]
        }
    }
}
